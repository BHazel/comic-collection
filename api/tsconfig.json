{
    "compilerOptions": {
        "target": "ES2016",
        "module": "CommonJS",
        "rootDir": "src",
        "outDir": "dist",
        "allowJs": true,
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "skipLibCheck": true,
        "sourceMap": true,
        "strict": false,
        "strictNullChecks": true
    }, "include": [
        "src/**/*"
    ], "exclude": [
        "jest.config.js"
    ]
}
